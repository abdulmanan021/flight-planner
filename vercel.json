{
    "builds": [
      {
        "src": "/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/src",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/frontend/src/$1" },
      { "src": "/api/(.*)", "dest": "backend/app.js" }
    ]
  }
  